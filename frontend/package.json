{
  "name": "ebnis",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nps",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@kanmii/socket": "^0.2.2",
    "@kanmii/socket-apollo-link": "^0.2.3",
    "apollo-cache-persist-dev": "^0.2.1",
    "broadcast-channel": "^3.3.0",
    "bulma": "^0.9.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "fuzzysort": "^1.1.4",
    "graphql": "^15.4.0",
    "immer": "^7.0.14",
    "lodash": "^4.17.20",
    "phoenix": "^1.5.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "uuid": "^8.3.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@cypress/webpack-preprocessor": "^5.4.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.164",
    "@types/node": "^12.0.0",
    "@types/phoenix": "^1.5.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/uuid": "^8.3.0",
    "apollo": "^2.31.0",
    "babel-loader": "^8.1.0",
    "cypress": "^5.5.0",
    "env-cmd": "^10.1.0",
    "netlify": "^4.9.0",
    "netlify-cli": "^2.67.2",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "nps": "^5.10.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "shelljs": "^0.8.4",
    "start-server-and-test": "^1.11.5",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "cypress/**/*.ts"
        ],
        "rules": {
          "jest/expect-expect": "off",
          "jest/valid-expect-in-promise": "off",
          "jest/valid-expect": "off"
        }
      },
      {
        "files": [
          "**/*.tsx"
        ],
        "rules": {
          "react/prop-types": "off",
          "import/no-anonymous-default-export": [
            "warn",
            {
              "allowArray": false,
              "allowArrowFunction": true,
              "allowAnonymousClass": false,
              "allowAnonymousFunction": true,
              "allowCallExpression": true,
              "allowLiteral": false,
              "allowObject": false
            }
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts*",
      "!src/__tests__/**",
      "!src/graphql/**",
      "!src/register-service-worker.*",
      "!src/react-app-env.d.ts"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules*",
      "<rootDir>/package.json",
      "<rootDir>/build/",
      "<rootDir>/coverage/",
      "<rootDir>/public/",
      "<rootDir>/.netlify/"
    ]
  }
}
