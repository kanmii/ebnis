{
  "name": "@ebnis/cra",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "eject": "react-scripts eject",
    "start": "nps"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.tsx"
        ],
        "rules": {
          "import/no-anonymous-default-export": [
            "warn",
            {
              "allowArray": false,
              "allowArrowFunction": true,
              "allowAnonymousClass": false,
              "allowAnonymousFunction": true,
              "allowCallExpression": true,
              "allowLiteral": false,
              "allowObject": false
            }
          ],
          "react/prop-types": "off"
        }
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts*",
      "!src/__tests__/**",
      "!src/graphql/**",
      "!src/register-service-worker.*",
      "!src/react-app-env.d.ts"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules*",
      "<rootDir>/package.json",
      "<rootDir>/build/",
      "<rootDir>/coverage/",
      "<rootDir>/public/"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@ebnis/commons": "0.0.0",
    "@ebnis/jsx": "0.0.0",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@kanmii/socket": "^0.2.2",
    "@kanmii/socket-apollo-link": "^0.2.3",
    "apollo-cache-persist-dev": "^0.2.1",
    "broadcast-channel": "^3.3.0",
    "bulma": "^0.9.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "fuzzysort": "^1.1.4",
    "graphql": "^15.4.0",
    "immer": "8.0.1",
    "lodash": "^4.17.20",
    "phoenix": "^1.5.6",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "uuid": "^8.3.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.164",
    "@types/node": "^12.0.0",
    "@types/phoenix": "^1.5.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/uuid": "^8.3.0",
    "env-cmd": "^10.1.0",
    "node-fetch": "^2.6.1",
    "nps": "^5.10.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "serve": "^11.3.2",
    "shelljs": "^0.8.4",
    "typescript": "^4.0.5"
  }
}
